{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testPathIgnorePatterns": ["/node_modules/", "/dist/"],
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": ["src/**/*.{ts,tsx}", "!**/node_modules/**"]
}