{
  "name": "backend",
  "root": "apps/backend",
  "sourceRoot": "apps/backend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "apps/backend"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run develop",
        "cwd": "apps/backend"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "eslintConfig": "apps/backend/eslint.config.mjs",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "cwd": "apps/backend/src",
        "command": "npm run lint",
        "lintFilePatterns": [
          "apps/backend/src/**/*.js"
        ],
        "fix": true
      }
    }
  },
  "tags": []
}