{
    "name": "backend",
    "root": "apps/backend",
    "sourceRoot": "apps/backend/src",
    "projectType": "application",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npm run build",
          "cwd": "apps/backend"
        }
      },
      "serve": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npm run develop",
          "cwd": "apps/backend"
        }
      },
      "lint": {
        "executor": "@nrwl/linter:eslint",
        "options": {
          "lintFilePatterns": ["apps/backend/**/*.js"]
        }
      }
    },
    "tags": []
  }
  