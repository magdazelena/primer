{
  "name": "backend",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "dev": "strapi develop",
    "build": "strapi build",
    "deploy": "strapi deploy",
    "build:all": "npm run build:plugins && strapi build",
    "build:plugins": "npm run build:strapi-plugin-product-actions && npm run build:strapi-plugin-status-manager",
    "build:strapi-plugin-product-actions": "cd ../../packages/strapi-plugin-product-actions && npm run build",
    "build:strapi-plugin-status-manager": "cd ../../packages/strapi-plugin-status-manager && npm run build",
    "start": "strapi start",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .strapi-updater.json build .turbo node_modules",
    "seed": "npm run build:all && strapi import -f ./seed-data.tar.gz",
    "export": "strapi export --no-encrypt --file seed-data",
    "strapi": "strapi",
    "generate-types": "strapi ts:generate-types"
  },
  "dependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.7.7",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.0.3",
    "@primershop/strapi-plugin-product-actions": "^0.0.15",
    "@primershop/strapi-plugin-status-manager": "^0.0.23",
    "@strapi/plugin-cloud": "^5.34.0",
    "@strapi/plugin-users-permissions": "^5.34.0",
    "@strapi/strapi": "^5.34.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.4.3",
    "better-sqlite3": "^12.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.22.3",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^10.0.1",
    "@jest/globals": "^30.0.5",
    "@strapi/types": "^5.34.0",
    "@types/node": "^25.0.2",
    "eslint": "9.39.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0"
  },
  "strapi": {
    "uuid": "0f95dc7c-202b-4ab2-a874-3799d23b1640"
  }
}
