{
  "name": "primer",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "apps/*",
    "cli"
  ],
  "scripts": {
    "backend": "nx dev backend",
    "cleanup": "nx clear frontend",
    "dev": "yarn cleanup && nx frontend backend",
    "frontend": "nx dev frontend",
    "lint": "nx run backend:lint && nx run frontend:lint",
    "setup": "yarn install",
    "prune": "rm -rf node_modules/ && rm -rf dist/ && rm .nx/ && yarn install",
    "test": "nx run-many --target=test  --projects=frontend,backend --parallel --passWithNoTests",
    "test:watch": "nx run-many --target=test --projects=frontend,backend --parallel --watch",
    "test:coverage": "nx run-many --target=test --coverage --projects=frontend,backend --parallel",
    "e2e": "nx run-many --target=e2e --projects=frontend"
  },
  "dependencies": {
    "@strapi/strapi": "^5.11.0",
    "eslint": "^9.22.0",
    "npm-check-updates": "^17.1.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@formatjs/intl-localematcher": "^0.6.0",
    "@heroicons/react": "^2.2.0",
    "@strapi/blocks-react-renderer": "^1.0.2",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^22.13.1",
    "@types/qs": "^6.9.18",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "classnames": "^2.5.1",
    "negotiator": "^1.0.0",
    "next": "^15.2.3",
    "qs": "^6.14.0",
    "react-icons": "^5.3.0",
    "remark-gfm": "^4.0.1",
    "typescript": "^5.3.3",
    "yet-another-react-lightbox": "^3.21.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.56.0",
    "@nx/eslint": "20.4.6",
    "@nx/jest": "20.4.6",
    "@nx/next": "20.4.6",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^15.2.0",
    "nx": "20.5.0",
    "@testing-library/cypress": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.6.6",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.6"
  },
  "packageManager": "yarn@4.5.2",
  "nx": {}
}
