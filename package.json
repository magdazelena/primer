{
  "name": "primer",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "cli"
  ],
  "scripts": {
    "backend": "nx dev backend",
    "cleanup": "nx clear frontend",
    "dev": "yarn cleanup && nx frontend backend",
    "e2e": "nx run-many --target=e2e --projects=frontend",
    "frontend": "nx dev frontend",
    "lint": "nx run backend:lint && nx run frontend:lint",
    "prune": "rm -rf node_modules/ && rm -rf dist/ && rm .nx/ && yarn install",
    "setup": "yarn install",
    "test:all": "nx run-many --target=test --parallel --passWithNoTests",
    "test:coverage": "nx run-many --target=test --coverage --projects=frontend,backend --parallel",
    "test:watch": "nx run-many --target=test --projects=frontend,backend --parallel --watch"
  },
  "dependencies": {
    "@strapi/strapi": "^5.11.0",
    "eslint": "^9.22.0",
    "npm-check-updates": "^17.1.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.56.0",
    "@nx/eslint": "20.5.0",
    "@nx/jest": "20.5.0",
    "@nx/js": "20.5.0",
    "@nx/next": "20.5.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^15.2.0",
    "nx": "20.5.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2"
  },
  "packageManager": "yarn@4.5.2",
  "nx": {}
}
