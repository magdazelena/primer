{
  "name": "primer",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "apps/*",
    "packages/*",
    "cli"
  ],
  "scripts": {
    "backend": "turbo dev --filter=backend",
    "cleanup": "turbo clean --filter=frontend",
    "dev": "turbo dev",
    "frontend": "turbo dev --filter=frontend",
    "lint": "turbo lint",
    "setup": "yarn install",
    "prune": "rm -rf node_modules/ && rm -rf dist/ && rm -rf .turbo/ && rm -rf package-lock.json",
    "test": "turbo test -- --passWithNoTests",
    "test:watch": "turbo test --watch",
    "test:coverage": "turbo test --coverage",
    "e2e": "turbo e2e",
    "deps:check": "turbo run deps:check",
    "deps:update": "turbo run deps:update",
    "dependabot:config": "node scripts/dependabot-config.js"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@types/jest": "^30.0.0",
    "eslint": "9.34.0",
    "jest": "^30.0.5",
    "npm-check-updates": "^18.0.2",
    "ts-jest": "^29.4.1",
    "turbo": "latest",
    "typescript": "^5.3.3"
  },
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "0f95dc7c-202b-4ab2-a874-3799d23b1640"
  }
}
