{
  "name": "primer",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "apps/*",
    "cli"
  ],
  "scripts": {
    "backend": "nx dev backend",
    "cleanup": "nx clear frontend",
    "dev": "yarn cleanup && nx frontend backend",
    "frontend": "nx dev frontend",
    "lint": "nx run backend:lint && nx run frontend:lint",
    "setup": "yarn install",
    "prune": "rm -rf node_modules/ && rm -rf dist/ && rm .nx/ && yarn install",
    "test": "nx run-many --target=test  --projects=frontend,backend --parallel --passWithNoTests",
    "test:watch": "nx run-many --target=test --projects=frontend,backend --parallel --watch",
    "test:coverage": "nx run-many --target=test --coverage --projects=frontend,backend --parallel",
    "e2e": "nx run-many --target=e2e --projects=frontend"
  },
