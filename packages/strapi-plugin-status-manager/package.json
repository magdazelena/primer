{
  "name": "@primer/strapi-plugin-status-manager",
  "version": "0.0.1",
  "description": "Enables more status variations for Strapi",
  "exports": {
    "./strapi-admin": {
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "strapi-server.js"
  ],
  "scripts": {
    "build": "npm-run-all clean --parallel build:code build:types",
    "build:code": "rollup -c",
    "build:dev": "rollup -c rollup.config.dev.mjs",
    "build:types": "run-p build:types:server build:types:admin",
    "build:types:server": "tsc -p server/tsconfig.build.json ",
    "build:types:admin": "tsc -p admin/tsconfig.build.json ",
    "clean": "rimraf ./dist",
    "lint": "eslint .",
    "test:front": "cross-env IS_EE=true jest --config ./jest.config.front.js",
    "test:ts:front": "tsc -p admin/tsconfig.json",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "watch": "rollup -c -w",
    "dev": "NODE_ENV=development DEBUG=true rollup -c rollup.config.dev.mjs -w",
    "test:debug": "NODE_ENV=development DEBUG=true node test-debug.js"
  },
  "keywords": [
    "strapi",
    "plugin",
    "status",
    "manager"
  ],
  "author": "Magda Odrowaz-Zelezik <magda.zelezik@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/primer.git",
    "directory": "packages/strapi-plugin-status-manager"
  },
  "bugs": {
    "url": "https://github.com/your-username/primer/issues"
  },
  "homepage": "https://github.com/your-username/primer/tree/main/packages/strapi-plugin-status-manager#readme",
  "peerDependencies": {
    "@strapi/strapi": "^5.22.0"
  },
  "devDependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.7.7",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.0.3",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-swc": "^0.4.0",
    "@strapi/admin": "^5.23.6",
    "@strapi/content-manager": "^5.23.6",
    "@strapi/types": "^5.23.6",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/node": "^24.7.0",
    "cross-env": "^10.0.0",
    "eslint": "9.38.0",
    "jest": "^30.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "kind": "plugin",
    "name": "primer-status-manager",
    "displayName": "Status Manager",
    "description": "Manages content statuses"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.27",
    "@strapi/icons": "^2.0.0-rc.27",
    "@strapi/utils": "^5.22.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.22.3",
    "styled-components": "^6.1.19"
  }
}
