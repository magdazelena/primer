{
  "name": "@primer/strapi-plugin-status-manager",
  "version": "0.0.1",
  "description": "Enables more status variations for Strapi",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "admin",
    "server"
  ],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./strapi-admin": {
      "source": "./src/admin/index.ts",
      "import": "./dist/admin/index.js",
      "require": "./dist/admin/index.cjs",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "source": "./src/server/index.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs",
      "default": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "build:strapi": "strapi-plugin build",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "strapi",
    "plugin",
    "status",
    "manager"
  ],
  "author": "Magda Odrowaz-Zelezik <magda.zelezik@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/primer.git",
    "directory": "packages/strapi-plugin-status-manager"
  },
  "bugs": {
    "url": "https://github.com/your-username/primer/issues"
  },
  "homepage": "https://github.com/your-username/primer/tree/main/packages/strapi-plugin-status-manager#readme",
  "peerDependencies": {
    "@strapi/strapi": "^5.13.1"
  },
  "devDependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.5.2",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.0.3",
    "@strapi/sdk-plugin": "^5.3.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "kind": "plugin",
    "name": "primer-status-manager",
    "displayName": "Status Manager",
    "description": "Enables more status variations"
  },
  "dependencies": {
    "@strapi/design-system": "^1.19.0",
    "@strapi/icons": "^1.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "6.30",
    "styled-components": "^5.3.11"
  }
}
